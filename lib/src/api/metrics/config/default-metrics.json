                    "type": "floatField",
                    "config": {
                        "lineWidth": 1,
                        "barWidth": 20,
                        "area": {
                            "show": true,
                            "opacity": 50
                        },
                        "type": "line",
                        "color": "#0991B3"
                    }
                }
            ],
            "config": {
                "line": {
                    "marker": {
                        "show": false
